<style type="text/css">
	body {
	  -webkit-print-color-adjust:exact;
	}
	.tbl_blk {
		color: #fff;
		background-color: #000;
		background-image: (images/1x1.gif)
	}
	.navbar {
		display: none;
	}
	.hd_r {
		border-radius: 10px;
		background-color: #000;
		color: #fff;
		font-size: 9px;
		padding-left: 10px;
		background-image: (app/images/1x1.gif)
	}

	@media print {
        .hd_r {
                border-radius: 10px;
                background-color: #000 !important;
                color: #fff;
                font-size: 9px;
                padding-left: 10px;
                -webkit-print-color-adjust: exact;
        	}
	 .hideb {
        	display: none;
        	padding-bottom:40px;
        }
	
}
</style>

<div class="row"> 
<div class="hideb"><a href="#/subscriber/report" class="btn">Back</a> <button ng-click="onPrint();" class="btn">Print</button></div>
	<div style="width:600px;margin:0 auto;">
		
		<div style="margin-bottom:10px;border-bottom:1px solid #444;page-break-inside: avoid;" ng-repeat="sub in subs">
			<div class="hd_r">
				<img style="padding:10px;" src="http://www.4zzzfm.org.au/sites/default/files/zed_logo.png"  width="160px;">
				<ul style="margin-bottom: 0px;" class="unstyled inline">
					<li>Creative Broadcasters Ltd </li>
					<li>264 Barry Pde </li>
					<li>PO Box 509 Fortitude Valley</li>
					<li>Ph:   (07) 3252 1555</li>
					<li>info@4zzz.org.au</li>		
				</ul>
				<ul class="unstyled inline">
					<li>ABN: 62 009 950 958</li>
					<li>Fortitude Valley</li>
					<li>Brisbane Australia 4006</li>
					<li>Fax: (07) 3252 1950</li>
					<li>www.4zzzfm.org.au</li>		
				</ul>                       
			</div>
			<div style="text-align:center;"><strong>Creative Broadcasters Ltd</strong><br/>
			Trading as &quot;4ZZZ FM&quot;<br/>
			ABN â€“ 62 009 950 958<br/>

			</div>

			<table class="table table-bordered table-condensed">
				<tr></tr>
				<tr><td colspan="2"><strong>Date:</strong> {{today}}</td></tr>
				<tr><td colspan="2"><strong>Received From:</strong> {{sub.subfirstname}} {{sub.sublastname}}</td></tr>
				<tr><td colspan="2">{{sub.subaddress1}} <br/>{{sub.suburb.suburb}} <br/> {{sub.suburb.state}}  {{sub.suburb.postcode}}</td></tr>
				<tr class="tbl_blk"><td>Item</td><td>Posted</td></tr>

				<tr><td style="text-align:right;">{{sub.subscription.subtypecode}}</td><td style="text-align:right;"> {{sub.subscription.subtypevalue | currency}}</td></tr>
				<tr><td style="text-align:right;">Total Amount Charged</td><td style="text-align:right;">{{sub.subscription.subtypevalue | currency}}</td></tr>
				<tr><td style="text-align:right;">Donation</td><td style="text-align:right;"> {{sub.donation | currency}}</td></tr>
				<tr><td style="text-align:right;">Postage</td><td style="text-align:right;"> {{sub.postage | currency}}</td></tr>
				<tr><td style="text-align:right;">GST Included</td><td style="text-align:right;"> {{sub.subscription.subtypevalue/11 | currency}}</td></tr>
			</table>


			<p style="text-align:center;">DONATIONS OF $2.00 OR MORE ARE TAX DEDUCTIBLE.<br/>
			SUBSCRIPTIONS ARE NOT TAX DEDUCTIBLE.</p>
			</div>
		
	</div>

</div>
